                              else
                              {
                                 xpathInstance2 = tempxpathInstance2.substring(tempxpathInstance2.lastIndexOf("/artikelList[1]")).substring("]".indexOf("]")).trim();
                              }
                            }
                            log.info("instance2 ===========" + xpathInstance2);

                        // add instance 1 xpath and instance 2 xpath to listXpath
                          listXpathInstance.add(0,xpathInstance1)
                          listXpathInstance.add(1,xpathInstance2)
                          locationXMLFileOf2Instance.add(0,temporaryXmlFile)
                          locationXMLFileOf2Instance.add(1,temporaryXmlFile2)

                    //Compare current subject, if already existed then throw it, if not add to listIssue
                        if (listIssue.size() > 0)
                        {
                            log.info("============= Start check if subject already existed in listIssue ==========================")
                              for (int indexListIssue = 0; indexListIssue < listIssue.size(); indexListIssue++ )
                              {
                                log.info(" --------- Comparing between new issue " + subject + " with listIssue at index " + indexListIssue + " " + listIssue.get(indexListIssue))
                                if (subject.equals(listIssue.get(indexListIssue)))
                                  {
                                        isIssueExistedInListIssue = true
                                        indexIssueExistedInListIssue = indexListIssue
                                        log.info("..... this issue  " + subject + " already existed in listIssue at index " + indexListIssue + ".......")
                                        log.info(".... Issue in listIssue at index " + indexListIssue + " is " + listIssue.get(indexListIssue))
                                        indexIssueExistedInListIssue = indexIssueExistedInListIssue + 1
                                        // ======================Check duplicate Id in lstIdResult 09 August 2018==============
                                        boolean isIdExistedInLstId = false
                                        for(int indexId = 0; indexId < lstIdResult.size(); indexId++)
                                        {
                                            if(indexIssueExistedInListIssue == lstIdResult.get(indexId))
                                              {
                                                isIdExistedInLstId = true
                                              }
                                        }
                                        if (isIdExistedInLstId == false)
                                          {
                                              lstIdResult.add(indexIssueExistedInListIssue)
                                          }

                                         break
                                  }
                                isIssueExistedInListIssue = false
                                log.info("..... this issue  " + subject + " is added to the list ..........")
                              }
                          }

                                  if(isIssueExistedInListIssue == false)
                                  {
                                    listIssue.add(subject)
                                    int collumnIndexToWriteIssue2Instance = 2
                                    log.info("=======Start finding Cell index which doesn't have data yet=======")
                                    int IndexIssueExcel = 2
                                    while (!sheetIssues.getCell(1,IndexIssueExcel).getContents().toString().equals(""))
                                      {
                                             IndexIssueExcel = IndexIssueExcel + 1
                                      }
                                    String idResult = Integer.toString(IndexIssueExcel - 1)
                                    lstIdResult.add(idResult);
                                    for (int w = 0; w < locationXMLFileOf2Instance.size(); w++)
                                    {
                                                cellIdResult = new Label(0, IndexIssueExcel, idResult);
                                                cellResultsubject = new Label(1, IndexIssueExcel,subject);
                                                sheetIssues.addCell(cellResultsubject)
                                                // Add ID into ID coloumn in sheet Issues
                                                sheetIssues.addCell(cellIdResult)
                                                // Add ID of issue Into list issue.
                                                //log list xpath isntance 1 and 2
                                                log.info("instance" + w +  " in listXpathInstance xpath is " + listXpathInstance.get(w))
                                                if (listXpathInstance.get(w).equals("null"))
                                                {
                                                      log.info("start write null to collumn " + collumnIndexToWriteIssue2Instance)
                                                      cellXpathNull = new Label(collumnIndexToWriteIssue2Instance, IndexIssueExcel, "null")
                                                      sheetIssues.addCell(cellXpathNull)
                                                      collumnIndexToWriteIssue2Instance++
                                                }
                                              else
                                                {
                                                // Parse Xpath to Xml format
                                                String locationXMLFileOf2InstanceFinal = locationXMLFileOf2Instance.get(w).replace('////', '//');
                                                File inputFile = new File(locationXMLFileOf2Instance.get(w))
                                                DocumentBuilderFactory dbFactory = DocumentBuilderFactory.newInstance()
                                                dbFactory.setNamespaceAware(true)
                                                DocumentBuilder dBuilder
                                                dBuilder = dbFactory.newDocumentBuilder()
                                                Document doc = dBuilder.parse(inputFile)
                                                XPath xPath =  XPathFactory.newInstance().newXPath()
                                                String expression = "/" + listXpathInstance.get(w)
                                                log.info("xpath expression " + w + " " + expression)
                                                NodeList nodeList = (NodeList) xPath.compile(expression.trim()).evaluate(doc, XPathConstants.NODESET);
                                                Document newXmlDocument = DocumentBuilderFactory.newInstance()
                                                        .newDocumentBuilder().newDocument();
                                                Element root = newXmlDocument.createElement("root")
                                                newXmlDocument.appendChild(root);
                                                for (int i = 0; i < nodeList.getLength(); i++)
                                                 {
                                                    org.w3c.dom.Node node = nodeList.item(i);
                                                    org.w3c.dom.Node copyNode = newXmlDocument.importNode(node, true)
                                                    root.appendChild(copyNode)
                                                 }
                                                  // Write XML issue to Excel
                                                 printXmlDocument(newXmlDocument,sheetIssues, cellFormat, IndexIssueExcel, collumnIndexToWriteIssue2Instance);
                                                 collumnIndexToWriteIssue2Instance++
                                                }
                                    }
                          }
                  //}
                  //log check listIssue

                    }
            }
  }
              // =================== END REFACTORING=======================
                                            log.info("***** Check list issue data  ******")
                              for(int indexListIssue = 0; indexListIssue < listIssue.size(); indexListIssue++ )
                                {
                                  log.info("Data at index " + indexListIssue + " is " + listIssue.get(indexListIssue))
                                }
          }
}
              else
          {
             errCompare = temporaryNoteFirstInstance;
            temporaryList = mapResult.get("$testStepName")
            temporaryList.set(3, temporaryNoteFirstInstance);
            mapResult.put("$testStepName", temporaryList)
            temporaryNoteFirstInstance = ""
          }

          }

    // Write result to ouput Excel file
      cellResult = new Label(indexHeaderResult, indexExcel,strFileCompareTestResult);
      sheetResult.addCell(cellResult)
      log.info("err ============" + err)
      log.info("strFileCompareTestResult  ============  " + strFileCompareTestResult)
      if(err == null)
      {
          cellResultNotes = new Label(indexHeaderResult + 1, indexExcel, errCompare);
      }
      if(strFileCompareTestResult == "BLOCKED")
      {
          // ---=========NEW=========Release 1045 SPC - Refactor the SPC reporting==============
            cellResultNotes = new Label(indexHeaderResult + 1, indexExcel, "Blocked at company \"$companyName\", line number $indexExcel, reason: \"$err1 get from the first instance \"$strFirstInstance\"");
      }
          if(strFileCompareTestResult == "FAILED")
          {
            log.info("strFileCompareTestResult = failed + listIssue " + lstIdResult.toString())
            cellResultNotes = new Label(indexHeaderResult + 1, indexExcel, "Please refer to sheet Issues with ID = #" + lstIdResult.toString());
          }
    sheetResult.addCell(cellResultNotes);

    log.info " ${text*6} End testing for test Step: $testStepName for company TD No " + companyTDKey + " at Excel data line # " +  indexExcel;
    log.info " ${text*100}";
    indexRunner = indexRunner + 1
    indexHeaderResult =  indexHeaderResult + 2

    temporaryIndex = indexRowReportSummaryOfTestStep + 3 + indexExcel + wsdlTestSteps.size()
    int subNumber = Math.abs(diffPerfElapsedTimeStep2nd - diffPerfElapsedTimeStep1st)
    int averageNumber = (diffPerfElapsedTimeStep2nd + diffPerfElapsedTimeStep1st) / 2
    int differPercentage = (subNumber/averageNumber) * 100

    if(differPercentage >= intLimitOfDifferentPercentage)
    {
        perfElapsedTimeDetails = new Label(temporaryIntForPerformanceTestStepDetails, temporaryIndex , temporaryStrPerfElapsedTimeStep, cellFormatHighlight);
    }
    else
    {
        perfElapsedTimeDetails = new Label(temporaryIntForPerformanceTestStepDetails, temporaryIndex , temporaryStrPerfElapsedTimeStep, cellFormat);
    }
        sheetPerformance.addCell(perfElapsedTimeDetails);
     temporaryIntForPerformanceTestStepDetails = temporaryIntForPerformanceTestStepDetails + 1
}

